---
import LayoutMenuNegocio from "../layouts/LayoutMenuNegocio.astro";
import Nav from "../components/Nav.astro";
import "../styles/global.css"
import BodyMenuNegocio from "../components/BodyMenuNegocio.astro";
---

<LayoutMenuNegocio>
    <Nav/>
    <br>
    <BodyMenuNegocio/>
</LayoutMenuNegocio>


<script>
    const userid = sessionStorage.getItem("Id");
const role = sessionStorage.getItem("Role");




    
if (role !== "profesional") {
        location.href = "/";
    }
    if (!userid) {
        location.href = "/";
    }
    
    // console.log('Id: ', userid);
    // console.log('Role: ', role);
    
</script>